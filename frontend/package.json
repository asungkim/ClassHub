    "@types/react-dom": "19.0.1",
    "autoprefixer": "10.4.20",
    "eslint": "9.14.0",
    "eslint-config-next": "16.0.3",
    "openapi-fetch": "^0.15.0",
    "openapi-typescript": "^7.10.1",
    "postcss": "8.4.47",
    "tailwindcss": "4.1.17",
    "typescript": "5.8.3"
